{
  "manifest_version": 2,
  "name": "SpeedVideoYT",
  "version": "1.0.2",
  "icons": {
    "128": "icon_128x128.png",
    "64": "icon_64x64.png",
    "16": "icon_16x16.png"
  },
  "content_scripts": [
    {
      "js": [ "jquery-3.5.1.min.js", "content.js" ],
      "matches": [ "https://www.youtube.com/*" ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [ "*.png" ]
}